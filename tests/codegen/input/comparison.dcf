callout printf;
void main() {
    printf("0: %d\n", 0 < 0);
    printf("0: %d\n", 1 < 0);
    printf("1: %d\n", -1 < 0);

    printf("0: %d\n", 0 > 0);
    printf("1: %d\n", 1 > 0);
    printf("0: %d\n", -1 > 0);

    printf("1: %d\n", 0 <= 0);
    printf("0: %d\n", 1 <= 0);
    printf("1: %d\n", -1 <= 0);

    printf("1: %d\n", 0 >= 0);
    printf("1: %d\n", 1 >= 0);
    printf("0: %d\n", -1 >= 0);
}
